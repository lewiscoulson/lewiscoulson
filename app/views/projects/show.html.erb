<h1 class="project">
  <%= @project.name %>
</h1>

<div>
  <%= @project.date %>
</div>

<div>
  <%= @project.skills %>
</div>

<div>
  <%= @project.images %>
</div>

<div>
  <%= @project.placeholder %>
</div>

<% if user_signed_in? %>
  <%= link_to('Edit', edit_project_path) %>
  <%= link_to('Delete', project_path(@project), method: :delete, data: {confirm: 'are you sure?'}) %>
<% end %>